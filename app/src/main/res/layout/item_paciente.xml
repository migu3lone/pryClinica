<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginRight="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="16dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:cardCornerRadius="15dp"
        app:cardElevation="8dp">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:padding="10dp">
        <ImageView
            android:id="@+id/imageLeft"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:src="@drawable/logo_usuario"
            android:layout_below="@id/vr2"
            android:layout_alignParentStart="true"
            android:layout_margin="10dp"/>

        <TextView
            android:id="@+id/vr1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/imageLeft"
            android:maxLines="1"
            android:paddingEnd="30dp"
            android:paddingBottom="5dp"
            android:textSize="18sp"
            android:gravity="center"
            android:layout_gravity="center"/>

        <TextView
            android:id="@+id/vr2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:maxLines="1"
            android:paddingEnd="30dp"
            android:paddingBottom="5dp"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/vr3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="18sp"
            android:paddingEnd="30dp"
            android:paddingBottom="5dp"
            android:layout_below="@id/vr1"
            android:gravity="center"
            android:layout_gravity="center"/>

        <ImageView
            android:id="@+id/editar"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:src="@drawable/editar"
            android:textSize="18sp"
            android:paddingEnd="30dp"
            android:paddingBottom="5dp"
            android:layout_below="@id/vr2"
            android:layout_toEndOf="@id/imageLeft"/>

        <ImageView
            android:id="@+id/eliminar"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:src="@drawable/eliminar"
            android:textSize="18sp"
            android:paddingEnd="30dp"
            android:paddingBottom="5dp"
            android:layout_below="@id/vr2"
            android:layout_toEndOf="@id/editar"/>

        <TextView
            android:id="@+id/vr4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="18sp"
            android:paddingEnd="30dp"
            android:paddingBottom="5dp"
            android:layout_below="@id/editar"
            android:layout_toEndOf="@id/imageLeft"/>

        <TextView
            android:id="@+id/vr5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="18sp"
            android:paddingEnd="30dp"
            android:paddingBottom="5dp"
            android:layout_below="@id/vr4"
            android:layout_toEndOf="@id/imageLeft"/>

        <TextView
            android:id="@+id/vr6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="18sp"
            android:paddingBottom="5dp"
            android:maxLines="1"
            android:layout_below="@id/vr5"
            android:layout_toEndOf="@id/imageLeft"/>

        <TextView
            android:id="@+id/vr7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="18sp"
            android:paddingBottom="5dp"
            android:maxLines="1"
            android:layout_below="@id/vr6"
            android:layout_toEndOf="@id/imageLeft"/>

        <TextView
            android:id="@+id/vr8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="18sp"
            android:paddingBottom="5dp"
            android:layout_below="@id/vr7"
            android:layout_toEndOf="@id/imageLeft"/>
    </RelativeLayout>

    </androidx.cardview.widget.CardView>






</androidx.constraintlayout.widget.ConstraintLayout >
